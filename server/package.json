{
  "name": "scriptocol-server",
  "version": "1.0.0",
  "description": "GitHub App using Probot",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "audit": "npm audit",
    "prepare": "husky install",
    "docker:dev": "docker-compose up",
    "docker:build": "docker build -t code-analyzer ."
  },
  "dependencies": {
    "@babel/parser": "^7.22.16",
    "@babel/traverse": "^7.22.15",
    "acorn": "^8.10.0",
    "axios": "^1.5.0",
    "body-parser": "^2.0.0-beta.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.0.0",
    "node-cache": "^5.1.2",
    "openai": "^4.10.0",
    "probot": "^13.0.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
